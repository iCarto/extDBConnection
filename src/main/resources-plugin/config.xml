<?xml version="1.0" encoding="ISO-8859-1"?>
<plugin-config>
	<depends plugin-name="org.gvsig.app.mainplugin"/>
	<depends plugin-name="org.gvsig.geodb.app.mainplugin"/>
	<depends plugin-name="es.icarto.gvsig.commons"/>
	<libraries library-dir="lib"/>
	<resourceBundle name="text"/>
	<extensions>
	
		<extension class-name="es.udc.cartolab.gvsig.users.PostGISDBConnectionExtension" active="true">
			<action 
                name= "es.udc.cartolab.gvsig.users.PostGISDBConnectionExtension"
                label="es.udc.cartolab.gvsig.users.PostGISDBConnectionExtension"
                tooltip="es.udc.cartolab.gvsig.users.PostGISDBConnectionExtension.tooltip"
                icon="es.udc.cartolab.gvsig.users.PostGISDBConnectionExtension"
                position="1"
                action-command=""
            />
            <menu text="data_base_menu/es.udc.cartolab.gvsig.users.PostGISDBConnectionExtension"
				name= "es.udc.cartolab.gvsig.users.PostGISDBConnectionExtension"
			/> 
			<tool-bar name="dbconnection">
				<action-tool name= "es.udc.cartolab.gvsig.users.PostGISDBConnectionExtension"/>
			</tool-bar>
		</extension>
		
		<extension class-name="es.udc.cartolab.gvsig.users.CloseSessionExtension" active="true">
			<action 
                name= "es.udc.cartolab.gvsig.users.CloseSessionExtension"
