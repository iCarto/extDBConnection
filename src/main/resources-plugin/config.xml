                position="3"
                action-command=""
            />
		
			<menu text="data_base_menu/close_session"
				name= "es.udc.cartolab.gvsig.users.CloseSessionExtension"
			/> 
				
			<tool-bar name="dbconnection">
				<action-tool name="es.udc.cartolab.gvsig.users.CloseSessionExtension" />
			</tool-bar>
		</extension>
		
		<extension class-name="es.udc.cartolab.gvsig.users.ChangePassExtension" active="true">
			<action 
                name= "es.udc.cartolab.gvsig.users.ChangePassExtension"
                label="es.udc.cartolab.gvsig.users.ChangePassExtension"
                tooltip="es.udc.cartolab.gvsig.users.ChangePassExtension"
